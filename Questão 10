import java.util.*;
class Main {
  public static void main(String[] args) {
    Scanner entrada = new Scanner(System.in);
    //fa√ßa um programa que leia um numero e que calcule e imprima o fatorial deste numero 
    int n,fat=1;
    int cont=1;
    do{
    System.out.println("Informe um numero:");
    n = entrada.nextInt();

    for (int i = 1; i <= n; i ++){
      fat = fat*i;
  }

    System.out.println("!" + n +"=" + fat);
    cont ++;

  }while (cont < 2);
}
}
  
